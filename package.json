{
  "name": "cypress-test",
  "version": "1.0.0",
  "description": "An Cypress e2e test project",
  "main": "index.js",
  "scripts": {
    "cy": "./node_modules/.bin/cypress",
    "cy:open": "npm run cy open",
    "test": "npm run cy run -- -q --headed",
    "cy:headless": "npm run cy run -- -q",
    "cy:calculator": "npm run cy run -- --spec specs/calculatorSpec.js",
    "ci": "npm run cy run -- -q"
  },
  "keywords": [
    "cypress",
    "e2e",
    "ui",
    "test",
    "automation"
  ],
  "author": "Dharendra",
  "license": "MIT",
  "devDependencies": {
    "cypress": "^4.12.1",
    "cypress-xpath": "^1.6.0"
  }
}
